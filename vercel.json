{
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "/api/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap.xml"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.html)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
